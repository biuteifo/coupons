{
  "/api": {
    "target": "http://localhost:8080",
    "logLevel": "info",
    "pathRewrite": {
      "^/api": "http://localhost:8080"
    }
  },
  "/login/*": {
    "target": "http://localhost:8080",
    "logLevel": "info"
  },
  "/socket/*": {
    "target": "http://localhost:8080",
    "logLevel": "error",
    "secure": false,
    "ws":true
  }
}
